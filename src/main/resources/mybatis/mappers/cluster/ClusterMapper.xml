  <select id= "countUserByType" parameterType="map" resultType="java.lang.Integer"  >
    <![CDATA[
        SELECT count(*)
          FROM `cluster_user` t, account c
         WHERE t.`clu_id` = #{gid,jdbcType=BIGINT} 
         AND t.`acc_id` = c.`id`
         AND CASE WHEN t.`acc_type` IS NULL THEN c.`type` ELSE t.`acc_type` END = #{type,jdbcType=BIGINT}
    ]]>
  </select>
  
