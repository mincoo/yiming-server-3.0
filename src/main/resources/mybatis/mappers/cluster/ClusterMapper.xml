  <select id= "searchBySn" parameterType="com.uxiaoxi.ym.appserver.web.cluster.vo.ClusterSearchBySnForm" resultMap="VOResultBySnMap" >
      <![CDATA[ 
        SELECT c.`id` AS gid,
               c.`create_by` AS uid,
               c.`sn` AS sn,
               c.`title` AS `name`,
               c.`face` AS `face`,
               c.`province` AS `province`,
               c.`city` AS `city`,
               c.`district` AS `district`,
               c.`school` AS `school`,
               t.`acc_name` AS `unamef`,
               m.`name` AS `unames`
          FROM `cluster_user` t,`cluster` c, account m
         WHERE t.`clu_id` = c.`id`
           AND t.`acc_id` = m.`id`
           AND c.`sn` =  #{sn,jdbcType=VARCHAR}
           AND t.`acc_id` = c.`create_by`
       ]]>
