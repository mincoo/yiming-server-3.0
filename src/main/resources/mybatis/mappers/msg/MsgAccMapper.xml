    `select2`,t.`selected` AS `selected`,m.`content` AS `content`
  </sql>
  
  <sql id="Base_Msg_condition">
    FROM msg_acc t, msg m , cluster c, `account` a
         WHERE t.`msg_id` = m.`id`
           AND t.`clu_id` = c.`id`
           AND m.`sender_id` = a.`id`
  </sql>
  
    <select id="getlist" parameterType="com.uxiaoxi.ym.appserver.web.msg.form.MsgListForm" resultType="com.uxiaoxi.ym.appserver.web.msg.vo.MsgListVO">
        SELECT DISTINCT
        <include refid="Base_Msg_List" />
        ,t.`received` AS `received`
        FROM msg_acc t, msg m , cluster c, `account` a
         WHERE t.`msg_id` = m.`id`
           AND t.`clu_id` = c.`id`
           AND m.`sender_id` = a.`id`
           AND t.`use_yn` = 1
           AND t.`acc_id` = #{uid,jdbcType=BIGINT}
           <if test="version != null and version !=0">
                AND t.`version` > #{version,jdbcType=BIGINT}
           </if>
           <if test="version ==0">
