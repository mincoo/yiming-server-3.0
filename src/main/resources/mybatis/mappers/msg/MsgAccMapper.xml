    <select id="getDataAcc" parameterType="java.lang.Long" resultType="com.uxiaoxi.ym.appserver.web.msg.vo.MsgDataPatInfo">
      SELECT ma.`acc_id` AS `sid`, 
             a.name AS `sname`, 
             ma.`selected` AS `selected` 
        FROM `msg_acc` ma 
        LEFT JOIN `account` a ON ma.`acc_id` = a.`id`
        LEFT JOIN `cluster_user` cu ON cu.`acc_id` = a.`id` AND cu.`clu_id` = ma.`clu_id`
       WHERE ma.`msg_id` = #{mid,jdbcType=BIGINT}
       AND CASE WHEN cu.`acc_type` IS NULL THEN a.`type` ELSE cu.`acc_type` END = 2
  </select> 
  
